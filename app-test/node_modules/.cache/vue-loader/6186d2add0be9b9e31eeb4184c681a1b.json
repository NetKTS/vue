\r\n            this.$emit('editInForm:Employee',edit)\r\n            document.getElementById(\"EditName\").value = \"\";\r\n            document.getElementById(\"EditEmail\").value = \"\";\r\n        },\r\n        cancleBtn(){\r\n            document.getElementById(\"EditName\").value = \"\";\r\n            document.getElementById(\"EditEmail\").value = \"\";\r\n            this.$emit('cancle')\r\n        }\r\n    }\r\n    \r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]}]}