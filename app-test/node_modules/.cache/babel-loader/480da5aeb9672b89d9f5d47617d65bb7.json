              <label> temperature in celsius </label>\r\n                <input type=\"text\" v-model=\"ce\">\r\n                <button @click=\"calfahrenheit()\">convert</button>\r\n                <h3> {{celsius}}c = {{fahrenheit}}f</h3>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: 'Lesson_5',\r\n    props: {\r\n    test: String\r\n  },\r\n    created(){\r\n        setInterval(() => {\r\n        this.seconds++;\r\n        },1000);\r\n},\r\n    data:function(){\r\n        \r\n        return{\r\n        seconds: 0,\r\n        text:\"\",\r\n        result:0,\r\n        a:0,\r\n        b:0,\r\n        celsius:0,\r\n        fahrenheit:0\r\n        }\r\n        \r\n    },\r\n    methods:{\r\n        sum(){\r\n            this.result = this.a + this.b\r\n        },\r\n        calfahrenheit(){\r\n            this.fahrenheit = this.celsius * (9 / 5) + 32;\r\n        }\r\n        \r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.textred{\r\n    color: red;\r\n}\r\n</style>"],"sourceRoot":""}]}